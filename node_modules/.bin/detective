#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/detective/bin/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/detective/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules:/home/ling/workarea/techarge-projects/node_modules:/home/ling/workarea/node_modules:/home/ling/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/detective/bin/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/detective/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules:/home/ling/workarea/techarge-projects/node_modules:/home/ling/workarea/node_modules:/home/ling/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detective/bin/detective.js" "$@"
else
  exec node  "$basedir/../detective/bin/detective.js" "$@"
fi
