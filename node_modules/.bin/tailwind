#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/tailwindcss@2.2.19_833e1018ad0d7954aa80c53675939269/node_modules/tailwindcss/lib/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/tailwindcss@2.2.19_833e1018ad0d7954aa80c53675939269/node_modules/tailwindcss/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/tailwindcss@2.2.19_833e1018ad0d7954aa80c53675939269/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules:/home/ling/workarea/techarge-projects/node_modules:/home/ling/workarea/node_modules:/home/ling/node_modules:/home/node_modules:/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/tailwindcss/lib/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/tailwindcss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/tailwindcss@2.2.19_833e1018ad0d7954aa80c53675939269/node_modules/tailwindcss/lib/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/tailwindcss@2.2.19_833e1018ad0d7954aa80c53675939269/node_modules/tailwindcss/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/tailwindcss@2.2.19_833e1018ad0d7954aa80c53675939269/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules:/home/ling/workarea/techarge-projects/node_modules:/home/ling/workarea/node_modules:/home/ling/node_modules:/home/node_modules:/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/tailwindcss/lib/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/tailwindcss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
