#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.4.6/node_modules/postcss-cli/bin/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.4.6/node_modules/postcss-cli/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.4.6/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules:/home/ling/workarea/techarge-projects/node_modules:/home/ling/workarea/node_modules:/home/ling/node_modules:/home/node_modules:/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/postcss-cli/bin/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/postcss-cli/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.4.6/node_modules/postcss-cli/bin/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.4.6/node_modules/postcss-cli/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.4.6/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/.pnpm/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules:/home/ling/workarea/techarge-projects/node_modules:/home/ling/workarea/node_modules:/home/ling/node_modules:/home/node_modules:/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/postcss-cli/bin/node_modules:/home/ling/workarea/techarge-projects/techarge.github.io/node_modules/postcss-cli/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../postcss-cli/bin/postcss" "$@"
else
  exec node  "$basedir/../postcss-cli/bin/postcss" "$@"
fi
